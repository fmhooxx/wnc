.uni-popup {
  position: fixed;
  top: 0px;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 8;
  overflow: hidden;
}
.uni-popup__mask {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 8;
    background: rgba(0, 0, 0, 0.4);
    opacity: 0;
}
.uni-popup__mask.ani {
      -webkit-transition: all 0.3s;
      transition: all 0.3s;
}
.uni-popup__mask.uni-top, .uni-popup__mask.uni-bottom, .uni-popup__mask.uni-center {
      opacity: 1;
}
.uni-popup__wrapper {
    position: absolute;
    z-index: 9;
    box-sizing: border-box;
}
.uni-popup__wrapper.ani {
      -webkit-transition: all 0.3s;
      transition: all 0.3s;
}
.uni-popup__wrapper.top {
      top: 0;
      left: 0;
      width: 100%;
      -webkit-transform: translateY(-100%);
              transform: translateY(-100%);
}
.uni-popup__wrapper.bottom {
      bottom: 0;
      left: 0;
      width: 100%;
      -webkit-transform: translateY(100%);
              transform: translateY(100%);
}
.uni-popup__wrapper.center {
      width: 100%;
      height: 100%;
